<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="BetterTextView">

        <attr name="jianEllipsize" format="enum">
            <enum name="start" value="0" />
            <enum name="middle" value="1" />
            <enum name="end" value="2" />
        </attr>
        <attr name="jianFontFamily" format="integer" />
        <attr name="jianMaxLines" format="integer" />
        <attr name="jianStrokeColor" format="color" />
        <attr name="jianText" format="string" />
        <attr name="jianTextColor" format="color" />
        <attr name="jianTextSize" format="dimension" />

    </declare-styleable>

</resources>